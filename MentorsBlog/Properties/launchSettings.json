{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "MentorsBlog": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "prod-remote"
      },
      "dotnetRunMessages": "true",
      "applicationUrl": "http://localhost:3460"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}